{
  "name": "openscraping",
  "version": "0.3.1",
  "description": "Turn unstructured HTML pages into structured data. The OpenScraping library can extract information from HTML pages using a JSON config file with xPath rules. It can scrape even multi-level complex objects such as tables and forum posts.",
  "main": "index.js",
  "dependencies": {
    "jsdom": "latest",
    "moment": "latest",
    "xmldom": "latest",
    "xmlserializer": "latest",
    "xpath": "latest"
  },
  "devDependencies": {
    "async": "latest",
    "chai": "latest",
    "eslint": "latest",
    "eslint-config-google": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-standard": "latest",
    "mocha": "latest",
    "mocha-eslint": "latest"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout 20000"
  },
  "author": "Ovidiu Dan <contact@ovidiudan.com> (https://www.ovidiudan.com/)",
  "license": "ISC",
  "tonicExampleFilename": "example.js",
  "engines": {
    "node": ">=0.4"
  },
  "bugs": {
    "url": "https://github.com/OpenScraping/openscraping-lib-nodejs/issues",
    "email": "contact@ovidiudan.com"
  },
